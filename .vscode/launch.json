{
    "version": "0.2.0",
    "configurations": [
        
    {
        "name": "(gdb) Pipe Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/ej_2/build/ej_2",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/ej_2",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }

}